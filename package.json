{
	"name": "gitcord",
	"version": "1.0.0",
	"description": "🪝Custom GitHub embeds for Discord",
	"author": "Daan Klarenbeek <github@ijskoud.dev>",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"homepage": "https://github.com/ijsKoud/project-template",
	"imports": {
		"#discord/*": "./dist/discord/*",
		"#github/*": "./dist/github/*",
		"#database/*": "./dist/database/*",
		"#shared/*": "./dist/lib/*"
	},
	"scripts": {
		"build": "tsc --build",
		"build:watch": "tsc --watch",
		"start:dev": "NODE_ENV=development nodemon ./dist",
		"start": "node ./dist",
		"lint": "eslint src",
		"postinstall": "is-ci || husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-angular": "^17.6.1",
		"@sapphire/eslint-config": "^4.4.1",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^4.0.0",
		"@types/eventsource": "^1.1.11",
		"@types/express": "^4.17.17",
		"@types/node": "^18.16.0",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.2.1",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"prisma": "^4.13.0",
		"typescript": "5.0.4"
	},
	"engines": {
		"node": ">= v18.16.0"
	},
	"packageManager": "yarn@3.5.0",
	"dependencies": {
		"@octokit/webhooks": "^11.0.0",
		"@octokit/webhooks-types": "^6.11.0",
		"@prisma/client": "^4.13.0",
		"@sapphire/discord-utilities": "^3.0.2",
		"@snowcrystals/icicle": "^1.0.4",
		"@snowcrystals/iglo": "^1.2.2",
		"colorette": "2.0.20",
		"discord.js": "^14.9.0",
		"dotenv": "^16.0.3",
		"eventsource": "^2.0.2",
		"express": "^4.18.2",
		"zod": "^3.21.4"
	}
}
